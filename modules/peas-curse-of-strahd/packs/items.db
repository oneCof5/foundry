bal","command":"// requires Warpgate, Sequencer, and JB2A patreon module\nconst selected = canvas.tokens.get(args.at(-1).tokenId); //canvas.tokens.controlled[0];\nconst stepSpell = args.at(-1).item\nconst distanceAvailable = stepSpell.system.range.value;\n\n// Warpgate Crosshairs\nlet crosshairsDistance = 0;\nconst checkDistance = async (crosshairs) => {\n    while (crosshairs.inFlight) {\n        await warpgate.wait(100); //wait for initial render\n        const ray = new Ray(selected.center, crosshairs);\n        const distance = canvas.grid.measureDistances([{ ray }], { gridSpaces: true })[0];\n\n        //only update if the distance has changed\n        if (crosshairsDistance !== distance) {\n            crosshairsDistance = distance;\n            if (distance > distanceAvailable) {\n                crosshairs.icon = 'icons/svg/hazard.svg';\n            } else {\n                crosshairs.icon = stepSpell.img;\n            }\n            crosshairs.draw();\n            crosshairs.label = `${distance} ft`;\n        }\n    }\n}\n\n// swap between targeting the grid square vs intersection based on token's size\nconst position = await warpgate.crosshairs.show({\n  interval: selected.document.width % 2 === 0 ? 1 : -1,\n  size: selected.document.width,\n  icon: stepSpell.img,\n  label: '0 ft.',\n  },{show: checkDistance});\n\nif (location.cancelled || crosshairsDistance > distanceAvailable) {return;}\n\nnew Sequence()\n  .effect()\n    .file(\"jb2a.misty_step.01.dark_green\") // 3 second animation\n    .atLocation(selected)\n    .belowTokens()\n    .scaleIn(0, 500, {ease: \"easeOutCubic\"})\n    .fadeOut(1000)\n    .scale({x:selected.document.width /4, y: selected.document.height/4})\n    .waitUntilFinished(-2999)\n\n  .animation()\n    .on(selected)\n    .opacity(0)\n    .fadeOut(250)\n    .waitUntilFinished()\n\n  .wait(250)\n \n  .animation()\n    .on(selected)\n    .teleportTo(position)\n    .snapToGrid()\n    .offset({ x: -1, y: -1 })\n    .waitUntilFinished()\n\n  .wait(250)\n\n  .effect()\n    .file(\"jb2a.misty_step.02.dark_green\") // 4 second animation\n    .atLocation(position)\n    .belowTokens()\n    .scaleIn(0, 500, {ease: \"easeOutCubic\"})\n    .fadeOut(1000)\n    .scale({x:selected.document.width /4, y: selected.document.height/4})\n    .waitUntilFinished(-3999)\n\n  .animation()\n    .on(selected)\n    .opacity(1)\n    .fadeIn(1000)\n    .waitUntilFinished()\n\n  .play()","author":"Jd73bpddRreBTuIB","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}},"scene-packer":{"sourceId":"Actor.5LUBbAQdm7gAVkCu.Item.uxJmJEXlShaUnrZX","hash":"0afb15bacf98e850a5c2cee06bc084e5717eb15c"}},"effects":[{"label":"Far Step","icon":"icons/magic/symbols/rune-sigil-black-pink.webp","origin":"Actor.vk6dV6GX5bNPbdNt.Item.uxJmJEXlShaUnrZX","duration":{"seconds":60,"startTime":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"disabled":false,"_id":"vVKd4eDShVpgVTQZ","changes":[{"key":"macro.createItem","mode":0,"value":"Compendium.peas-curse-of-strahd.items.RCcO2feSjJQFZ9wO","priority":20}],"tint":null,"transfer":false,"flags":{"dae":{"selfTarget":false,"selfTargetAlways":false,"stackable":"none","durationExpression":"","macroRepeat":"none","specialDuration":[]},"core":{"statusId":""},"ActiveAuras":{"isAura":false,"aura":"None","radius":"null","alignment":"","type":"","ignoreSelf":false,"height":false,"hidden":false,"displayTemp":false,"hostile":false,"onlyOnce":false},"dfreds-convenient-effects":{"description":""}}}],"img":"icons/magic/symbols/rune-sigil-black-pink.webp","ownership":{"default":0,"Jd73bpddRreBTuIB":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1666964194375,"modifiedTime":1678553385315,"lastModifiedBy":"Jd73bpddRreBTuIB"},"folder":null,"sort":0,"_id":"merlnbrjJW